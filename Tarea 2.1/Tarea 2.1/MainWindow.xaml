<Window x:Class="Tarea_2._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tarea_2._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <Menu Margin="0,10,0,0" HorizontalAlignment="Center" Width="780" Height="30" VerticalAlignment="Top">
            <MenuItem Header="_Vehiculo" Height="26" Width="68" FontSize="14">
                <MenuItem x:Name="listVehiculo" Header="Lista de Vehiculos" Click="listVehiculo_Click"/>
                <MenuItem x:Name="insertVehiculo" Header="Introducir Vehiculo" Click="insertVehiculo_Click"/>
                <MenuItem x:Name="updateVehiculo" Header="Modificar Vehiculo" Click="updateVehiculo_Click"/>
                <MenuItem x:Name="deleteVehiculo" Header="Eliminar Vehiculo" Click="deleteVehiculo_Click"/>
            </MenuItem>
            <MenuItem Header="_Cliente" Height="26" Width="71" FontSize="14" RenderTransformOrigin="1.648,0.694">
                <MenuItem x:Name="listCliente" Header="Lista de Clientes" Click="listCliente_Click"/>
                <MenuItem x:Name="insertCliente" Header="Introducir Cliente" Click="insertCliente_Click"/>
                <MenuItem x:Name="updateCliente" Header="Modificar Cliente" Click="updateCliente_Click"/>
                <MenuItem x:Name="deleteCliente" Header="Eliminar Cliente" Click="deleteCliente_Click"/>
            </MenuItem>
            <MenuItem Header="_Alquiler" Height="26" Width="71" FontSize="14" RenderTransformOrigin="1.648,0.694">
                <MenuItem x:Name="listAlquiler" Header="Lista de Alquileres" Click="listAlquiler_Click"/>
                <MenuItem x:Name="insertAlquiler" Header="Nuevo Alquiler" Click="insertAlquiler_Click"/>
                <MenuItem x:Name="updateAlquiler" Header="Modificar Alquiler" Click="updateAlquiler_Click"/>
                <MenuItem x:Name="deleteAlquiler" Header="Eliminar Alquiler" Click="deleteAlquiler_Click"/>
            </MenuItem>
        </Menu>
        <Frame x:Name="framePrincipal" Margin="0,59,0,0" HorizontalAlignment="Center" Width="714" Height="350" VerticalAlignment="Top" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>

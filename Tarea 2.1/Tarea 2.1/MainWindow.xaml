<Window x:Class="Tarea_2._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tarea_2._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <Menu Margin="0,10,0,0" HorizontalAlignment="Center" Width="780" Height="30" VerticalAlignment="Top">
            <MenuItem Header="Vehiculo" Height="26" Width="68" FontSize="14">
                <MenuItem x:Name="insertVehiculo" Header="Introducir Vehiculo" Click="insertVehiculo_Click"/>
            </MenuItem>
            <MenuItem Header="Cliente" Height="26" Width="71" FontSize="14" RenderTransformOrigin="1.648,0.694">
                <MenuItem x:Name="insertCliente" Header="Introducir Cliente" Click="insertCliente_Click"/>
            </MenuItem>
            <MenuItem Header="Alquiler" Height="26" Width="71" FontSize="14" RenderTransformOrigin="1.648,0.694">
                <MenuItem x:Name="insertAlquiler" Header="Nuevo Alquiler" Click="insertAlquiler_Click"/>
            </MenuItem>
        </Menu>
        <Frame x:Name="framePrincipal" Content="Frame" Margin="0,59,0,0" HorizontalAlignment="Center" Width="714" Height="350" VerticalAlignment="Top"/>
    </Grid>
</Window>
